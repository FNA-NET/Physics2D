{
    "version": "0.2.0",
    "configurations": [
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
            "name": "Launch Sample Hello World",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildSampleHelloWorld",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Physics2D.Samples.HelloWorld/bin/Debug/net6.0/Physics2D.Samples.HelloWorld.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Physics2D.Samples.HelloWorld/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Sample Testbed",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildSampleTestbed",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Physics2D.Samples.Testbed/bin/Debug/net6.0/Physics2D.Samples.Testbed.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Physics2D.Samples.Testbed/bin/Debug/net6.0",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}